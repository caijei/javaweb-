r\n          <el-col :span=\"22\" v-for=\"(tp, index) in listdata\" :key=\"index\" :offset=\"1\" class=\"mb-1 room-card\">\r\n            <el-card :body-style=\"{ padding: '0px' }\">\r\n              <el-row>\r\n                <el-col :span=\"6\">\r\n                  <img :src=\"tp.coverImage\" class=\"room-image\" />\r\n                </el-col>\r\n                <el-col :span=\"18\">\r\n                  <div style=\"padding: 14px;\" class=\"text-left room\">\r\n                    <p class=\"room-title\">{{ tp.typeName }}</p>\r\n                    <p class=\"room-intro\">{{ tp.introduce }}</p>\r\n                    <p class=\"room-price\">￥{{ tp.price }} 起</p>\r\n                    <p class=\"room-comment\">“{{ tp.feature }}”</p>\r\n                  </div>\r\n                </el-col>\r\n              </el-row>\r\n            </el-card>\r\n          </el-col>\r\n        </el-row>\r\n      </el-main>\r\n      <el-footer class=\"footer\">\r\n        <footbar></footbar>\r\n      </el-footer>\r\n      <el-dialog title=\"提示\" :visible.sync=\"dialogVisible\" width=\"95%\">\r\n        <span>您还没有选择完整的时间段！</span>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button type=\"primary\" @click=\"dialogVisible = false\"> 确定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </el-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport footbar from \"@/components/footbar.vue\";\r\nimport store from \"./../store\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      form: {\r\n        indate: \"\",\r\n        leavedate: \"\",\r\n        currentDate: new Date(),\r\n      },\r\n      listdata: {},\r\n    };\r\n  },\r\n  components: {\r\n    footbar,\r\n  },\r\n  methods: {\r\n    searchBtn() {\r\n      if (this.form.indate == \"\" || this.form.leavedate == \"\") {\r\n        this.dialogVisible = true;\r\n        return;\r\n      }\r\n      this.axios\r\n        .post(\"http://localhost:9151/user/listRoom\", {\r\n          inTime: this.changeTimeStr(this.form.indate),\r\n          leaveTime: this.changeTimeStr(this.form.leavedate),\r\n        })\r\n        .then((res) => {\r\n          this.$store.state.searchSet = res.data.data;\r\n          this.$store.state.searchTime.inTime = this.changeTimeStr(this.form.indate);\r\n          this.$store.state.searchTime.leaveTime = this.changeTimeStr(this.form.leavedate);\r\n          this.$router.push(\"/search\");\r\n        })\r\n        .catch(() => {\r\n          console.log(\"error\");\r\n        });\r\n    },\r\n    changeTimeStr(str) {\r\n      str = str.toString();\r\n      str = str.replace(/ GMT.+$/, \"\");\r\n      let d = new Date(str);\r\n      let a = [\r\n        d.getFullYear(),\r\n        d.getMonth() + 1,\r\n        d.getDate(),\r\n        d.getHours(),\r\n        d.getMinutes(),\r\n        d.getSeconds(),\r\n      ];\r\n      for (let i = 0; i < a.length; i++) {\r\n        if (a[i] < 10) a[i] = \"0\" + a[i];\r\n      }\r\n      return `${a[0]}-${a[1]}-${a[2]} ${a[3]}:${a[4]}`;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.axios\r\n      .get(\"http://localhost:9151/user/listTypes\")\r\n      .then((res) => {\r\n        this.listdata = res.data.data;\r\n      })\r\n      .catch((res) => {\r\n        console.log(res);\r\n      });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.hotel-name {\r\n  font-size: 1.6rem;\r\n  font-weight: bold;\r\n  color: #fff;\r\n  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.datepicker {\r\n  border-radius: 12px;\r\n}\r\n\r\n.search-btn {\r\n  background: linear-gradient(135deg, #1e90ff, #6a5acd);\r\n  color: white;\r\n  border-radius: 25px;\r\n  transition: transform 0.3s, box-shadow 0.3s;\r\n}\r\n\r\n.search-btn:hover {\r\n  transform: translateY(-3px);\r\n  box-shadow: 0 8px 15px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.room-card {\r\n  animation: fadeIn 1s ease-in-out;\r\n}\r\n\r\n.room-image {\r\n  width: 100%;\r\n  border-radius: 12px;\r\n  transition: transform 0.3s;\r\n}\r\n\r\n.room-image:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.room-title {\r\n  font-size: 1.4rem;\r\n  font-weight: bold;\r\n  color: #303133;\r\n}\r\n\r\n.room-intro {\r\n  font-size: 1rem;\r\n  color: #606266;\r\n}\r\n\r\n.room-price {\r\n  font-size: 1.4rem;\r\n  font-weight: bold;\r\n  color: #f56c6c;\r\n}\r\n\r\n.room-comment {\r\n  font-size: 0.9rem;\r\n  font-style: italic;\r\n  color: #909399;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n</style>\r\n"]}]}